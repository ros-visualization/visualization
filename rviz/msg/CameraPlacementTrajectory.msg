# When to start the camera placement trajectory
# (There is no header here because the frame is specified per-camera-placement.)
time stamp

# The vector of camera placements
rviz/CameraPlacement[] placements

# The name of the controller that should be activated before any movement happens. 
# Empty string will be interpreted as "no change"
string active_controller_at_start

# The name of the controller that should be activated when movement is done.
# Empty string will be interpreted as "no change"
string active_controller_at_end




